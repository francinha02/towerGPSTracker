    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js",
    "prod": "tsc && node ./dist/index.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
